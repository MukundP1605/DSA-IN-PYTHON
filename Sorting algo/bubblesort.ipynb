   "source": [
    "### Bubble sort "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3d283123",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 2, 4, 5, 7, 8, 9]\n"
     ]
    }
   ],
   "source": [
    "n = [1,2,5,4,7,8,9]\n",
    "swapped = False\n",
    "for i in range(0,len(n)):\n",
    "    for j in range(0,len(n)-i-1):\n",
    "        \n",
    "        if n[j+1] < n[j]:\n",
    "            n[j] , n[j+1] = n[j+1] , n[j]\n",
    "            swapped = True\n",
    "    if swapped == False:\n",
    "        break    \n",
    "print(n)      "
   ]
  }
 ],
